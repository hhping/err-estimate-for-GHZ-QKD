library verilog;
use verilog.vl_types.all;
entity log2_single is
    port(
        clk             : in     vl_logic;
        areset          : in     vl_logic;
        a               : in     vl_logic_vector(31 downto 0);
        q               : out    vl_logic_vector(31 downto 0);
        en              : in     vl_logic_vector(0 downto 0)
    );
end log2_single;
